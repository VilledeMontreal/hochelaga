            {{# module-news}}
            <h2>Dernières nouvelles</h2>
            <div class="content-module-stacked">
              {{> components-list-group-teaser}}
              <div class="d-flex justify-content-center">
              {{> components-button}}
              </div>
            </div>
            {{/ module-news}}

            {{# module-events}}
            <h2>Événements à venir</h2>
            <div class="content-module-stacked">
              {{> components-list-group-teaser}}
              <div class="d-flex justify-content-center">
              {{> components-button}}
              </div>
            </div>
            {{/ module-events}}

            {{# module-documents}}
            <h2>Documents (titre éditorial)</h2>
            <div class="content-module-stacked">
              {{> components-list-item-link}}
            </div>
            {{/ module-documents }}

            {{# module-list-reference}}
            <h2>Liste de références de liens internes ou externes</h2>
            <div class="content-module-stacked">
              {{> components-list-item-link }}
            </div>
            {{/ module-list-reference}}

            {{# module-texte-optionnel}}
            <h2>Module Titre de section</h2>
            <div class="content-module-stacked">
