                <li>Liste numérotée</li>
              </ol>
            </div>
            {{/ module-texte-optionnel}}

            {{# module-emphasis }}
            <div class="content-module-stacked">
            {{> styles-blockquote }}
            </div>
            {{/ module-emphasis}}
            
            {{# listAutoProgrammes}}
            <h2>Liste automatisée des programmes</h2>
            <div class="content-module-stacked">
              {{> components-list-item-link}}
            </div>
            {{/ listAutoProgrammes}}

            {{# listAutoArticles}}
            <h2>Liste automatisée des conseils-articles</h2>
            <div class="content-module-stacked">
              {{> components-list-item-link}}
            </div>
            {{/ listAutoArticles}}

            {{# listAutoProjets}}
            <h2>Liste automatisée des projets</h2>
            <div class="content-module-stacked">
              {{> components-list-item-link}}
            </div>
            {{/ listAutoProjets}}
          </div>
        </div>
        {{# sidebarRight}}
        <aside class="col-12 col-lg-4" role="complementary">
          {{> templates-citoyens-sidebar-unite-administrative}}
        </aside>
        {{/ sidebarRight}}

      </section>

      <section class="row{{# sidebarRight }} justify-content-between{{/ sidebarRight}}{{^ sidebarRight}} justify-content-center{{/ sidebarRight}}">
        <div class="col-12 col-lg-7">
        {{> components-feedback-form}}
        </div>
      </section>

    </div>
  </div>
</main>

{{> patterns-footer-portal }}
