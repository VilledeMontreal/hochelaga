{{> components-skip-navigation }}
<header class="header-main-nav {{# navbarFixedTop}}is-fixed-top{{/navbarFixedTop}}">
{{> components-navbar }}
</header>

<main class="main-content {{ pageClasses }}" id="main-content" aria-label="Main Content">
  {{# breadcrumb}}
  <div class="container pt-4 pt-lg-8">
    <div class="row">
      <div class="col-12">
        {{> components-fil-ariane}}
      </div>
    </div>
  </div>
  {{/ breadcrumb}}
  <div class="region-content {{# breadcrumb}}pt-0{{/breadcrumb}}">
    <div class="container">

      {{> patterns-content-header-profile}}

      <div class="row justify-content-between">
        <div class="col-12 col-lg-7">
          <div class="content-modules">
            {{# module-content}}
            {{# content}}
            <h2>{{ title }}</h2>
            {{# description}}
            <div class="content-module-stacked">{{{ description }}}</div>
            {{/ description}}
            <div class="content-module-stacked">
              {{> components-liste-contenu}}
            </div>
            {{/ content}}
            {{/ module-content}}
          </div>
        </div>

        {{# sidebarRight}}
        <aside class="col-12 col-lg-4" role="complementary">
          {{> templates-citoyens-sidebar-elu}}
        </aside>
        {{/ sidebarRight}}

      </div>

      <section class="row">
        <div class="col-12">
        {{> components-feedback-form}}
        </div>
      </section>

    </div>
  </div>
</main>

{{> patterns-footer-main }}
