{{# collapsibleItem }}
<div class="collapsible-item">
  <div class="collapsible-header" id="{{ heading-id }}">
    <h3 class="collapsible-title">
      {{^ item-disabled}}
      <button data-toggle="collapse" data-target="#{{ block-id }}" aria-expanded="{{# show}}true{{/ show}}{{^ show}}false{{/ show}}" aria-controls="{{ block-id }}">
        <span>{{ title }}</span>
        {{# subtitle}}<span class="collapsible-subtitle">{{subtitle}}</span>{{/ subtitle}}
      </button>
      {{/ item-disabled}}
      {{# item-disabled}}
      <span class="disabled" aria-expanded="false">
        <span>{{ title }}</span>
        {{# subtitle}}<span class="collapsible-subtitle">{{subtitle}}</span>{{/ subtitle}}
      </span>
      {{/ item-disabled}}
    </h3>
  </div>
  <div id="{{ block-id }}" class="collapse {{# show }}show{{/ show}}" role="region" aria-labelledby="{{ heading-id }}">
    {{# collapsible-default-content}}
    <div class="collapsible-block">
      {{{ collapsible-default-content }}}
    </div>
    {{/ collapsible-default-content}}
    {{# collapsible-item-canaux}}
    <div class="collapsible-block">
      {{> templates-citoyens-demarche-canaux}}
    </div>
    {{/ collapsible-item-canaux}}
    {{# collapsible-item-vdl311}}
    <div class="collapsible-block">
      {{> templates-outils-interne-collapse-vdl311}}
    </div>
    {{/ collapsible-item-vdl311}}
  </div>
</div>
{{/ collapsibleItem }}
